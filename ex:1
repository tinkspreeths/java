import java.io.*;
import java.util.*;
class Main {
   
   int i;
   int n;
   static  int  factorial=1; 
   public void fact()
   {
       for(i=1;i<=n;i++)
       {
           factorial=factorial*i;
       }
       System.out.println(factorial);
   }
	public static void main (String[] args) {
	    
	   Scanner sc=new Scanner(System.in);
	   Main f=new Main();
	   
   f.n = sc.nextInt();
   
   System.out.println("enter fact"+ " " +f.n);
   f.fact();
		
		
	}
}

enter fact 5                                                                                                                  
120   
